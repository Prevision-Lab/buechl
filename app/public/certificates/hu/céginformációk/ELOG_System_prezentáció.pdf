-o-animation-duration: 2s;
animation-duration: 2s;
}
/* Anpassung "letzte Posts" */
.zoom.wpb_single_image>div:hover>a>img,
.zoom.wpb_single_image>div:hover>img,
.ozy-owlcarousel .item.item-extended:hover>img,
.ozy-image-with-caption:hover>figure>img,
.ozy-fancyimagebox:hover>img {
  -moz-transform:scale(1.0) rotate(0deg);
  -webkit-transform:scale(1.0) rotate(0deg);
  -o-transform:scale(1.0) rotate(0deg);
  transform:scale(1.0) rotate(0deg);
}
/* Festes Höhen und Breitenverhältnis der letzten Posts */
.zoom.wpb_single_image>div>img, .zoom.wpb_single_image>div>a>img, .ozy-owlcarousel .item.item-extended>img, .ozy-image-with-caption>figure>img, .ozy-fancyimagebox>img {
  height: 300px !important;
  width: 370px !important;
}


/***** NEUIGKEITEN *****/
/* Anpassung News Grid */
.vc_grid-filter.vc_grid-filter-default > .vc_grid-filter-item {
background-color: #bbb;
}
.featured-thumbnail-header {
  height: 450px;
  max-width: 1212px;
  margin: 0px auto;
  background-position: top center;
}
/* Hover blau */
.vc_custom_1419253765784 {
  background-color: rgba(7,95,173,0.7) !important;
}
.newer-older-blog {
  display: none;
}
/* Meta ausblenden */
.post-meta.content-font {
  display: none !important;
}

.hide {
  display:none !important;
}

/***** ELEMENTE *****/
/***** TABS *****/
/* Tabs Styling */
.tabs nav a span {
text-transform: uppercase;
}
.wpb_content_element .wpb_tabs_nav li, .wpb_accordion .wpb_accordion_wrapper .wpb_accordion_header {
  border-color: #000000 !important;
}
/* Tabs Icons open/close */
/* Icon OPEN */
.wpb_accordion .wpb_accordion_wrapper .ui-state-default .ui-icon, .wpb_accordion .wpb_accordion_wrapper .ui-state-active .ui-icon {
  width: 18px;
  height: 18px;
  background-image: url(https://www.buechl.de/wp-content/uploads/2016/01/tab-arrow-open.png);
  left: 0.0em;
  top: 0.6em;
}
/* Icon CLOSE */
.wpb_accordion .wpb_accordion_wrapper .ui-state-active .ui-icon {
  background-image: url(https://www.buechl.de/wp-content/uploads/2016/01/tab-arrow-close.png);
}
/* Textanpassung */
.wpb_accordion .wpb_accordion_wrapper .wpb_accordion_header a {
  padding-left: 2.0em;
}


/* SLIDER */
/* prev + next ausblenden */
span.wpb_prev_slide {
  display: none;
}
span.wpb_next_slide {
  display: none;
}
/* Anpassung Teaser Boxen */
.ozy-simple-hove-box {
overflow: hidden;
}
.ozy-simple-hove-box>a {
  position: relative;
  display: block;
  height: 186px;
}
.ozy-simple-hove-box>a>img {
  max-width: 124%;
}


/* Bilder proportional skalieren */
.image-scale {
max-width: 100% !important;
height: auto !important;
max-height: 40px !important;
}
/* Seltsamer Abstand nach Soft-Return */
#content br {
margin-top: 0.0em !important;
}
/* Akkordeon Boxen */
.wpb_accordion .wpb_accordion_wrapper .wpb_accordion_header {
  border-color: rgba(0,0,0,1) !important;
  border: 0 !important;
  height: 38px;
}
.owl-carousel .owl-item {
  overflow: visible;
}
/* Firmengruppe-Slider */
div#all_around_slider_1 {
  margin-left: 15px;
}
.content_slider_text_block_wrap div {
  font-family: "Raleway";
  line-height: 1.5em;
  font-size: 14px;
}
.circle_slider_thumb {
  -webkit-filter: grayscale(1);
  filter: grayscale(1);
  opacity: 0.5;
}
.all_around_position_4 {
  -webkit-filter: grayscale(0);
  filter: grayscale(0);
  opacity: 1;
}
/* Call to Action Button */
.cta_align_left .wpb_button, .cta_align_right .wpb_button {
  max-width: 100%;
  background-color: #38ae5d;
}
/* Karriere Headline Textboxen */
.textboxzweizeilig {
  height: 25px;
}
/* Karriere Textboxen feste höhe */
.textboxdreizeilig {
  height: 55px;
}


/***** SPECIAL *****/
/* Anpassung Bildmotiv Geschäftsführung */
.vc_custom_1449154034680 {
  background-position: center top !important;
}


/***** KONTAKT *****/
/* Kontaktformular */
input.wpcf7-form-control.wpcf7-text {
  background-color: #f0f0f1;
}
textarea.wpcf7-form-control.wpcf7-textarea {
  background-color: #f0f0f1;
}
#request-a-rate input:not([type=submit]):not([type=file]):not([type=radio]):not([type=checkbox]), #request-a-rate textarea, #request-a-rate select, #content input:not([type=submit]):not([type=file]):not([type=radio]):not([type=checkbox]), #content textarea, #content select, #sidebar input:not([type=submit]):not([type=file]):not([type=radio]):not([type=checkbox]), #sidebar textarea, #sidebar select {
  text-transform: none;
  letter-spacing: 0px;
}


/***** FOOTER *****/
/* Anpassung Input Boxen */
input, textarea, select {
padding: 7px 16px;
}
#footer-widget-bar .widget input {
 margin: 10px 0px 10px 0px;
}

/* Full-width Footer */
.ozy-page-model-boxed #main {
  max-width: none;
  background: none;
}
#main>.container {
  background: #fff;
}
div#footer-widget-bar {
  max-width: 1212px;
  left: 50%;
  transform: translate(-50%, 0);
  -webkit-transform: translate(-50%, 0);
  -ms-transform: translate(-50%, 0);
  position: relative;
}
.firmen-links-footer {
  padding-bottom: 10px;
}

/* Hotfix Bildmotive */
.wpb_single_image img {
  height: auto;
  width: 100%;
}

/* News Monat und Jahr */
.specialDatumAnzeige {
  color: #FFFFFF;
}

/* Mobile Menu Fix */
@media (max-width: 1120px) {
.menu-button ~ #menu-hauptnavigation-hu {
    opacity: 0;
    transition: opacity 200ms ease-in;
}
.menu-button.opened ~ #menu-hauptnavigation-hu {
    opacity: 1;
}
#top-menu > ul {
  top: 95px !important;
}
} 
/* Bouncing Scroll-Link ausblenden */
a.row-botton-button.bounce {
  display:none;
}

#page-title-wrapper h3,  {
  font-weight: 700;
  font-size: 22px;
  line-height: 1.5em;
  color: #fff;
}
.h3-special {
   color:#fff;
   font-size:22px;
}



